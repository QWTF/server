void() Menu_RepeatHelp = 
{
	local string tfhelp;
	local string class_help; // if you have class help off you probably don't want intro menu
	
	class_help = infokey (self, "ch");
	if (class_help == string_null)
		class_help = infokey (self, "classhelp");
		
	tfhelp = infokey(self, "tf_help");
	
	if ( (tfhelp != "off") && (class_help != "off"))
	{
		CenterPrint2(self, "Press ö to see this help again\n", "New Command: 'mega' for Mega-TF Menu\n");
	}
};

