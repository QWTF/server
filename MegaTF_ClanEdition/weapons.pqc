	}
	r_num = random ();
	if ((r_num < 0.6))
	{
		x = '1 0 0';
		y = '0 1 0';
		v = ((x * (50 - (random () * 100))) + (y * (50 - (random () * 100))));
		r_num = random ();
		SpawnMeatSpray (self.origin, v);
	}
	else
	{
		if ((r_num >= 0.6))
		{
			ThrowGib ("progs/gib1.mdl", self.health);
		}
		else
		{
			ThrowGib ("progs/gib3.mdl", self.health);
		}
	}
};


